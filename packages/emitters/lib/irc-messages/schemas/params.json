{
  "$schema": "http://json-schema.org/draft-07/schema",
  "definitions": {
    "stringWithLength": {
      "minLength": 1,
      "type": "string"
    },
    "arrayOfUniqueStringsWithLength": {
      "items": {
        "$ref": "#/definitions/stringWithLength"
      },
      "minItems": 1,
      "uniqueItems": true,
      "type": "array"
    }
  },
  "required": [
    "channelPrefixes",
    "channels",
    "dangerousAcceptUnauthorized",
    "dangerousInsecure",
    "encoding",
    "nick",
    "port",
    "realName",
    "host",
    "userName"
  ],
  "properties": {
    "channelPrefixes": {
      "default": ["&", "#"],
      "$ref": "#/definitions/arrayOfUniqueStringsWithLength"
    },
    "channels": {
      "$ref": "#/definitions/arrayOfUniqueStringsWithLength"
    },
    "dangerousAcceptUnauthorized": {
      "default": false,
      "type": "boolean"
    },
    "dangerousInsecure": {
      "default": false,
      "type": "boolean"
    },
    "encoding": {
      "default": "utf-8",
      "$ref": "#/definitions/stringWithLength"
    },
    "localAddress": {
      "$ref": "#/definitions/stringWithLength"
    },
    "nick": {
      "$ref": "#/definitions/stringWithLength"
    },
    "password": {
      "$ref": "#/definitions/stringWithLength"
    },
    "port": {
      "type": "number",
      "minimum": 1,
      "default": 6697
    },
    "realName": {
      "default": "Finch IRC Bot",
      "$ref": "#/definitions/stringWithLength"
    },
    "host": {
      "$ref": "#/definitions/stringWithLength"
    },
    "userName": {
      "default": "finchIrcBot",
      "$ref": "#/definitions/stringWithLength"
    }
  },
  "type": "object"
}
