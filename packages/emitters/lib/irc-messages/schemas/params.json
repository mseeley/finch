{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": [
    "channelPrefixes",
    "channels",
    "dangerousCertExpired",
    "dangerousInsecure",
    "dangerousSelfSigned",
    "encoding",
    "nick",
    "password",
    "port",
    "realName",
    "server",
    "userName"
  ],
  "properties": {
    "channelPrefixes": {
      "type": "string",
      "minLength": 1,
      "default": "&#"
    },
    "channels": {
      "type": "array",
      "minItems": 1,
      "contains": {
        "type": "string",
        "minLength": 1
      }
    },
    "dangerousCertExpired": {
      "type": "boolean",
      "default": false
    },
    "dangerousInsecure": {
      "type": "boolean",
      "default": false
    },
    "dangerousSelfSigned": {
      "type": "boolean",
      "default": false
    },
    "encoding": {
      "type": "string",
      "default": "utf-8",
      "minLength": 1
    },
    "localAddress": {
      "type": "string",
      "minLength": 1
    },
    "nick": {
      "type": "string",
      "minLength": 1
    },
    "password": {
      "type": "string",
      "minLength": 1
    },
    "port": {
      "type": "number",
      "minimum": 1
    },
    "realName": {
      "type": "string",
      "minLength": 1,
      "default": "Finch IRC Bot"
    },
    "server": {
      "type": "string",
      "minLength": 1
    },
    "userName": {
      "type": "string",
      "minLength": 1,
      "default": "finchIrcBot"
    }
  }
}
